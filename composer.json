{
    "name": "garrettw/aspire-dic",
    "description": "Dependency Injection Container for the Aspire framework and anyone else who wants to use it",
    "type": "library",
    "keywords": ["php", "dic", "ioc", "container", "inversion of control", "dependency injection", "dice"],
    "homepage": "https://github.com/garrettw/aspire-dic",
    "license": "MIT",
    "authors": [
        {
            "name": "Garrett Whitehorn",
            "homepage": "http://garrettw.net/",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "behat/behat": "^3.2",
        "friendsofphp/php-cs-fixer": "^1.12.1",
        "phpdocumentor/phpdocumentor": "^2.9",
        "phpspec/phpspec": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Aspire\\DIC\\": "src/",
            "": "vendor/"
        }
    }
}
