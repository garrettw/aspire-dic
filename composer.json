{
    "name": "outboardphp/di",
    "description": "Dependency Injection Container for the Outboard framework and anyone else who wants to use it",
    "type": "library",
    "keywords": ["php", "dic", "ioc", "container", "inversion of control", "dependency injection", "dice"],
    "homepage": "https://github.com/outboardphp/di",
    "support": {
        "issues": "https://github.com/outboardphp/outboard/issues"
    },
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Garrett Whitehorn",
            "homepage": "https://garrettw.net/",
            "role": "Lead Developer"
        }
    ],
    "provide": {
        "psr/container-implementation": "1.0.0"
    },
    "require": {
        "php": ">=8.4",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "dg/bypass-finals": "^1.9",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^3.8",
        "phpcompatibility/php-compatibility": "@dev",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-mockery": "^2.0",
        "squizlabs/php_codesniffer": "^3.13.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "minimum-stability": "dev",
        "prefer-stable": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Outboard\\Di\\": "src/",
            "Outboard\\Di\\Tests\\": "tests/"
        }
    }
}
